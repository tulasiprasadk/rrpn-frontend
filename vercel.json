{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://rrw-backend.vercel.app/api/$1"
    },
    { "source": "/(.*)", "destination": "/" }
  ]
}
