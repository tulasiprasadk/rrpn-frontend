{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    }
  ],
  "rewrites": [
    { "source": "/api/(.*)", "destination": "https://rrnagar-backend-github-app.vercel.app/api/$1" },
    { "source": "/api", "destination": "https://rrnagar-backend-github-app.vercel.app/api" },
    { "source": "/(.*)", "destination": "/" }
  ]
}
